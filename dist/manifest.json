{
  "manifest_version": 3,
  "name": "Detrust",
  "version": "1.0",
  "description": "Browser extension to display Detrust scores in Twitter",
  "icons": {
    "16": "detrust-16.png",
    "32": "detrust-32.png",
    "48": "detrust-48.png",
    "128": "detrust-128.png"
  },
  "content_scripts": [
    {
      "js": ["index.js"],
      "matches": ["https://twitter.com/*"]
    }
  ]
}
